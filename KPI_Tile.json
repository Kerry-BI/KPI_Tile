{
	"id": "KPI.Tile",
	"version": "1.0.0",
	"name": "KPI Tile",
	"description": "A standard KPI tile",
	"newInstancePrefix": "KPItile",
	"vendor": "SAP",
	"eula": "",
	"license": "",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "com-sap-sample-kpi-tile",
			"url": "/main2.js",
			"integrity": "",
			"ignoreIntegrity": true
		}
	],
	"properties": {
		"color": {
			"type": "string",
			"description": "Background color",
			"default": "white"
		},
		"opacity": {
			"type": "number",
			"description": "Opacity",
			"default": 1
		},
		"width": {
			"type": "integer",
			"default": 100
		},
		"height": {
			"type": "integer",
			"default": 100
		}
	},
	"methods": {
		"setColor": {
			"description": "Sets the background color.",
			"parameters": [
				{
					"name": "newColor",
					"type": "string",
					"description": "The new background color"
				}
			],
			"body": "this.color = newColor;"
		},
		"getColor": {
			"returnType": "string",
			"description": "Returns the background color.",
			"body": "return this.color;"
		}
	},
	"events": {
		"onClick": {
			"description": "Called when the user clicks."
		}
	},    
	"dataBindings": {
		"myDataSource": {
		  "feeds": [
			{
			  "id": "dimensions",
			  "description": "Dimensions",
			  "type": "dimension"
			},
			{
			  "id": "measures",
			  "description": "Measures",
			  "type": "mainStructureMember"
			}
		  ]
		}
	  }
}

 